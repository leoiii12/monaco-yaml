{
  "compilerOptions": {
    "module": "umd",
    "moduleResolution": "node",
    "baseUrl": "src",
    "outDir": "out",
    "forceConsistentCasingInFileNames": true,
    "target": "es6",
    "lib": [
      "dom",
      "es5",
      "es2015.collection",
      "es2015.promise",
      "es2016",
      "es2017.string"
    ],
    "downlevelIteration": true
  }
}
