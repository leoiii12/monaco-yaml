{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "out",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es2017",
    "lib": [
      "dom",
      "es5",
      "es2015.collection",
      "es2015.promise",
      "es2016",
      "es2017.string"
    ],
    "downlevelIteration": true
  },
  "include": ["src/**/*"]
}
